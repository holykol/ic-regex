type Match = record {
    text: text;
    start: nat64;
    end: nat64;
};

type Re = text;

service : {
    "precompile": (vec Re) -> ();
    "purge_cache": () -> ();
    "is_match": (Re, text) -> (bool) query;
    "captures": (Re, text) -> (vec opt Match) query;
    "is_match_batch": (Re, vec text) -> (vec bool) query;
    "captures_batch": (Re, vec text) -> (vec vec opt Match) query;
};
